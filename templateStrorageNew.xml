	<!DOCTYPE r-pro>
	<r-pro version="1.0">
		<progname><!-- Имя программы --></progname>
		<comment><!-- Условный комментарий --></comment>
		<descriptive_part>
			<memory_block>
				<memory type="Memories.Register" name="reg1"/>
				<memory type="Memories.Register" name="Memories.Register"/>
				<memory type="Memories.Register" name="reg2"/>
				<memory type="Memories.Counter" name="ct1"/>
				<memory type="Memories.Counter" name="a"/>
				<memory type="Memories.Counter" name="b"/>
				<memory type="Memories.Wagon" leftName="W1LeftName" rightName="W1RightName"/>
				<memory type="Memories.Table" name = "tname">
					<columnsName>
						<column>Name1</column>
						<column>Name2</column>
					</columnsName>
				</memory>
			</memory_block>
			<alphabet>
				<abc name="Letter" short_name="L" description="descr"></abc>
				<abc name="Letter1" short_name="L1" description="descr1"></abc>
			</alphabet>
		</descriptive_part>
		<alg>
			<arm begin = "0">
                <edge end = "1">
                    <predicate type="alphabet">Letter</predicate>
					<operation>
						<left value = "ct1"/>
						<operator>-&gt;</operator>
						<right value ="reg1"/>
					</operation>
                </edge>
                <edge end = "2">
                    <predicate type="expression">
						<left value = "a"/>
						<sign>-&gt;</sign>
						<right value = "b"/>
					</predicate>
					<operation>
						<left value =  "reg1"/>
						<operator>&lt;-</operator>
						<right value = "Hello World"/>
					</operation>
					<operation>
						<left value =  "ct1"/>
						<operator>-&gt;</operator>
						<right value = "reg1"/>
					</operation>
                </edge>
				<edge end = "3">
					<predicate type="String">perdConst</predicate>
					<operation>
						<left value = "ct1"/>
						<operator>-&gt;</operator>
						<right value = "reg1"/>
					</operation>
					<operation>
						<left value =  "ct1"/>
						<operator>-&gt;</operator>
						<right value = "reg1"/>
					</operation>
				</edge>
				<edge end = "4">
					<predicate type="memory">reg1</predicate>
					<operation>
						<left value = "ct1"/>
						<operator>-&gt;</operator>
						<right value = "reg1"/>
					</operation>
				</edge>
            </arm>
		</alg>
	</r-pro>
